    "version": "0.2.0",
    "configurations": [
        


        
        {
            "name": "debug current",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "lambda_config",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolderBasename}/tools/lambda_config.py",
            "console": "integratedTerminal",
            "args": "--raw-payload 0x000001DEADFACE00AABBCCDDEEFF0000000000000000"
        },
        {
            "name": "lambda_mux",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolderBasename}/tools/lambda_mux.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": "--list"
        }
    ]
}